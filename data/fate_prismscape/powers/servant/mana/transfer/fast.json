{
    "type": "origins:action_over_time",
    "interval": 80,
    "entity_action": {
        "type": "origins:action_on_entity_set",
        "set": "fate_prismscape:contract/masters",
        "bientity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:target_action",
                    "action": {
                        "type": "origins:execute_command",
                        "command": "function fate_prismscape:mana/operation/subtract_1"
                    }
                },
                {
                    "type": "origins:actor_action",
                    "action": {
                        "type": "origins:execute_command",
                        "command": "function fate_prismscape:mana/operation/add_1"
                    }
                }
            ]
        },
        "bientity_condition": {
            "type": "origins:target_condition",
            "condition": {
                "type": "origins:xp_points",
                "comparison": ">",
                "compare_to": 0
            }
        }
    },
    "condition": {
        "type": "origins:predicate",
        "predicate": "fate_prismscape:relative_mana/low"
    },
    "name": "Fast Mana Transfer",
    "description": "You absorb mana more quickly from your Master when low on mana.",
    "hidden": true
}