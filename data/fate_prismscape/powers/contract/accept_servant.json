                            "type": "origins:play_sound",
                            "sound": "minecraft:block.beacon.power_select",
                            "volume": 16,
                            "pitch": 1
                        },
                        {
                            "type": "origins:execute_command",
                            "command": "origin set @s fate_prismscape:master fate_prismscape:master"
                        },
                        {
                            "type": "origins:action_on_entity_set",
                            "set": "fate_prismscape:contract/pending_set",
                            "bientity_action": {
                                "type": "origins:and",
                                "actions": [
                                    {
                                        "type": "origins:add_to_entity_set",
                                        "set": "fate_prismscape:contract/servants"
                                    },
                                    {
                                        "type": "origins:remove_from_entity_set",
                                        "set": "fate_prismscape:contract/pending_set"
                                    }
                                ]
                            }
                        }
                    ]
                },
                "else_action": {
                    "type": "origins:execute_command",
