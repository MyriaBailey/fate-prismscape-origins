{
    "type": "origins:resource",
    "min": -1,
    "max": 1,
    "start_value": 0,
    "max_action": {
        "type": "origins:and",
        "actions": [
            {
                "type": "origins:change_resource",
                "resource": "fate_prismscape:contract/cancel",
                "change": 0,
                "operation": "set"
            },
            {
                "type": "origins:if_else",
                "condition": {
                    "type": "origins:entity_set_size",
                    "set": "fate_prismscape:contract/pending_set",
                    "comparison": ">",
                    "compare_to": 0
                },
                "if_action": {
                    "type": "origins:and",
                    "actions": [
                        {
                            "type": "origins:execute_command",
                            "command": "tellraw @s \"Request canceled.\""
                        },
                        {
                            "type": "origins:action_on_entity_set",
                            "set": "fate_prismscape:contract/pending_set",
                            "bientity_action": {
                                "type": "origins:and",
                                "actions": [
                                    {
                                        "type": "origins:target_action",
                                        "action": {
                                            "type": "origins:execute_command",
                                            "command": "tellraw @s \"The contract request was canceled.\""
                                        }
                                    },
                                    {
                                        "type": "origins:remove_from_entity_set",
                                        "set": "fate_prismscape:contract/pending_set"
                                    }
                                ]
                            }
                        }
                    ]
                },
                "else_action": {
                    "type": "origins:execute_command",
                    "command": "tellraw @s \"Request timed out.\""
                }
            }
        ]
    },
    "name": "Cancel Contract",
    "description": "Undo sending a contract request.",
    "hidden": true
}